{
  "version": 2,
  "builds": [
    { "src": "api/*.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/$1" },
    { "src": "/(.*\\.css)", "dest": "/api/static.js?file=$1" },
    { "src": "/(.*\\.js)", "dest": "/api/static.js?file=$1" },
    { "src": "/(.*\\.png)", "dest": "/api/static.js?file=$1" },
    { "src": "/(.*\\.jpg)", "dest": "/api/static.js?file=$1" },
    { "src": "/", "dest": "/api/index.js" }
  ]
}